{
  "ai_providers": {
    "openai": {
      "base_url": "https://api.openai.com/v1",
      "models": {
        "gpt4": "gpt-4",
        "gpt4_turbo": "gpt-4-turbo-preview", 
        "gpt4o": "gpt-4o",
        "gpt4o_mini": "gpt-4o-mini",
        "gpt35_turbo": "gpt-3.5-turbo"
      },
      "endpoints": {
        "chat": "/chat/completions",
        "embeddings": "/embeddings",
        "audio": "/audio/speech"
      }
    },
    
    "anthropic": {
      "base_url": "https://api.anthropic.com",
      "models": {
        "claude3_opus": "claude-3-opus-20240229",
        "claude3_sonnet": "claude-3-sonnet-20240229",
        "claude3_haiku": "claude-3-haiku-20240307"
      },
      "endpoints": {
        "messages": "/v1/messages"
      }
    },
    
    "google_ai": {
      "base_url": "https://generativelanguage.googleapis.com",
      "models": {
        "gemini_pro": "gemini-pro",
        "gemini_vision": "gemini-pro-vision"
      },
      "endpoints": {
        "generate": "/v1/models/{model}:generateContent",
        "chat": "/v1/models/{model}:streamGenerateContent"
      }
    },
    
    "azure_openai": {
      "base_url": "https://{your-resource-name}.openai.azure.com",
      "api_version": "2024-02-01",
      "deployments": {
        "gpt4": "gpt-4-deployment",
        "gpt35_turbo": "gpt-35-turbo-deployment"
      },
      "endpoints": {
        "chat": "/openai/deployments/{deployment}/chat/completions"
      }
    },
    
    "groq": {
      "base_url": "https://api.groq.com/openai/v1",
      "models": {
        "llama3_8b": "llama3-8b-8192",
        "llama3_70b": "llama3-70b-8192",
        "mixtral": "mixtral-8x7b-32768"
      },
      "endpoints": {
        "chat": "/chat/completions"
      }
    },
    
    "huggingface": {
      "base_url": "https://api-inference.huggingface.co",
      "models": {
        "llama2": "meta-llama/Llama-2-7b-chat-hf",
        "mistral": "mistralai/Mistral-7B-Instruct-v0.2"
      },
      "endpoints": {
        "inference": "/models/{model}"
      }
    },
    
    "together_ai": {
      "base_url": "https://api.together.xyz/v1",
      "models": {
        "llama2_70b": "togethercomputer/llama-2-70b-chat",
        "codellama": "togethercomputer/CodeLlama-34b-Instruct"
      },
      "endpoints": {
        "chat": "/chat/completions"
      }
    }
  },
  
  "emailjs": {
    "user_public_key": "<your-emailjs-user-id>",
    "service_id": "<service_id>",
    "template_id": "<template_id>"
  },
  
  "app_config": {
    "default_provider": "openai",
    "default_model": "gpt-4o-mini",
    "timeout": 30000,
    "max_retries": 3,
    "cache_enabled": true
  },
  
  "security": {
    "use_env_vars": true,
    "allowed_origins": ["https://yourdomain.com"],
    "rate_limiting": {
      "enabled": true,
      "requests_per_minute": 60
    }
  },
  
  "notes": [
    "JANGAN commit file ini dengan API keys yang nyata",
    "Gunakan environment variables untuk sensitive data",
    "Copy file ini sebagai config.json dan isi dengan values yang sesuai", 
    "Pastikan config.json ada di .gitignore"
  ]
}